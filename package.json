    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "handlebars": "^4.7.7",
    "jest": "^29.6.3",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.1",
    "or": "^0.2.0",
    "otplib": "^12.0.1",
    "pg": "^8.10.0",
    "randomstring": "^1.2.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
